<?xml version="1.0" encoding="utf-8" ?>
<ValueProviders>

  <!-- Classic -->

  <OperationClass>
    <type>Binary</type>
    <syntax>+</syntax>
    <worker class="OperationWorker_Addition" />
    <priority>2</priority>
  </OperationClass>

  <OperationClass>
    <type>Binary</type>
    <syntax>-</syntax>
    <worker class="OperationWorker_Substraction" />
    <priority>2</priority>
  </OperationClass>

  <OperationClass>
    <type>Binary</type>
    <syntax>*</syntax>
    <worker class="OperationWorker_Multiplication" />
    <priority>10</priority>
  </OperationClass>

  <OperationClass>
    <type>Binary</type>
    <syntax>/</syntax>
    <worker class="OperationWorker_Division" />
    <priority>10</priority>
  </OperationClass>

  <OperationClass>
    <type>Binary</type>
    <syntax>^</syntax>
    <worker class="OperationWorker_Power" />
    <priority>100</priority>
  </OperationClass>

  <!-- exponential -->

  <OperationClass>
    <type>UnaryPrefix</type>
    <syntax>ln</syntax>
    <worker class="OperationWorker_NatLogarithm" />
    <priority>10010</priority>
  </OperationClass>

  <OperationClass>
    <type>UnaryPrefix</type>
    <syntax>lg</syntax>
    <worker class="OperationWorker_DecimalLogarithm" />
    <priority>10010</priority>
  </OperationClass>

  <!-- Trigonometrical -->

  <OperationClass>
    <type>UnaryPrefix</type>
    <syntax>sin</syntax>
    <worker class="OperationWorker_Sin" />
    <priority>10010</priority>
  </OperationClass>

  <OperationClass>
    <type>UnaryPrefix</type>
    <syntax>cos</syntax>
    <worker class="OperationWorker_Cos" />
    <priority>10010</priority>
  </OperationClass>

  <OperationClass>
    <type>UnaryPrefix</type>
    <syntax>tg</syntax>
    <worker class="OperationWorker_Tangent" />
    <priority>10010</priority>
  </OperationClass>

  <OperationClass>
    <type>UnaryPrefix</type>
    <syntax>ctg</syntax>
    <worker class="OperationWorker_Cotangent" />
    <priority>10010</priority>
  </OperationClass>

  <!-- Other -->

  <OperationClass>
    <type>UnaryPostfix</type>
    <syntax>!</syntax>
    <worker class="OperationWorker_Factorial" />
    <priority>10100</priority>
  </OperationClass>

  <!--Consts-->

  <ConstValue>
    <name>e</name>
    <value>2,71828182845904523536</value>
  </ConstValue>

  <ConstValue>
    <name>pi</name>
    <value>3,14159265358979323846</value>
  </ConstValue>

</ValueProviders>
